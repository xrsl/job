{
  "$defs": {
    "CareerPage": {
      "additionalProperties": false,
      "description": "Career page configuration for job search.",
      "properties": {
        "company": {
          "title": "Company",
          "type": "string"
        },
        "url": {
          "title": "Url",
          "type": "string"
        },
        "enabled": {
          "default": true,
          "title": "Enabled",
          "type": "boolean"
        },
        "keywords": {
          "items": {
            "type": "string"
          },
          "title": "Keywords",
          "type": "array"
        },
        "extra-keywords": {
          "items": {
            "type": "string"
          },
          "title": "Extra-Keywords",
          "type": "array"
        }
      },
      "required": [
        "company",
        "url"
      ],
      "title": "CareerPage",
      "type": "object"
    },
    "JobAdd": {
      "additionalProperties": false,
      "description": "Add command configuration.",
      "properties": {
        "browser": {
          "default": false,
          "title": "Browser",
          "type": "boolean"
        },
        "structured": {
          "default": false,
          "title": "Structured",
          "type": "boolean"
        },
        "model": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Model"
        }
      },
      "title": "JobAdd",
      "type": "object"
    },
    "JobApp": {
      "additionalProperties": false,
      "description": "App command configuration.",
      "properties": {
        "model": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Model"
        },
        "cv": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Cv"
        },
        "letter": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Letter"
        },
        "extra": {
          "items": {
            "type": "string"
          },
          "title": "Extra",
          "type": "array"
        }
      },
      "title": "JobApp",
      "type": "object"
    },
    "JobFit": {
      "additionalProperties": false,
      "description": "Fit assessment configuration.",
      "properties": {
        "cv": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Cv"
        },
        "model": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Model"
        },
        "extra": {
          "items": {
            "type": "string"
          },
          "title": "Extra",
          "type": "array"
        }
      },
      "title": "JobFit",
      "type": "object"
    },
    "JobGH": {
      "additionalProperties": false,
      "description": "GitHub integration configuration.",
      "properties": {
        "repo": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Repo"
        },
        "auto-assign": {
          "default": false,
          "title": "Auto-Assign",
          "type": "boolean"
        },
        "default-labels": {
          "items": {
            "type": "string"
          },
          "title": "Default-Labels",
          "type": "array"
        }
      },
      "title": "JobGH",
      "type": "object"
    },
    "JobSearch": {
      "additionalProperties": false,
      "description": "Job search configuration.",
      "properties": {
        "keywords": {
          "items": {
            "type": "string"
          },
          "title": "Keywords",
          "type": "array"
        },
        "parallel": {
          "default": false,
          "title": "Parallel",
          "type": "boolean"
        },
        "since": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Since"
        },
        "in": {
          "items": {
            "$ref": "#/$defs/CareerPage"
          },
          "title": "In",
          "type": "array"
        }
      },
      "title": "JobSearch",
      "type": "object"
    },
    "Settings": {
      "description": "Application settings loaded from job.toml and environment variables.\n\nPrecedence order:\n1. Environment variables (with JOB_ prefix)\n2. job.toml file (see _find_config_file for search order)\n3. Default values from schema\n\nExample environment variables:\n    JOB_VERBOSE=true\n    JOB_MODEL=gemini-2.5-flash\n    JOB_DB_PATH=~/my-jobs.db\n    JOB_FIT__CV=~/cv.md",
      "properties": {
        "model": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Model"
        },
        "verbose": {
          "default": false,
          "title": "Verbose",
          "type": "boolean"
        },
        "db-path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Db-Path"
        },
        "gh": {
          "$ref": "#/$defs/JobGH"
        },
        "fit": {
          "$ref": "#/$defs/JobFit"
        },
        "add": {
          "$ref": "#/$defs/JobAdd"
        },
        "app": {
          "$ref": "#/$defs/JobApp"
        },
        "search": {
          "$ref": "#/$defs/JobSearch"
        }
      },
      "title": "Settings",
      "type": "object"
    }
  },
  "description": "Root model for job.toml schema generation.",
  "properties": {
    "job": {
      "$ref": "#/$defs/Settings"
    }
  },
  "required": [
    "job"
  ],
  "title": "_JobToml",
  "type": "object"
}
